        "typeProperties": {
          "source": {
            "type": "SnowflakeSource",
            "query": "SELECT\r\nA.STUDENT_UNIQUE_ID AS \"student_unique_id\",\r\nA.STANDARDISED_ASSESSMENT_MARK_UNIQUE_ID AS \"standardised_assessment_mark_unique_id\",\r\nA.STANDARDISED_ASSESSMENT_MARK_ID AS \"standardised_assessment_mark_id\",\r\nA.APPLICATION_ID AS \"application_id\",\r\nA.STUDENT_ID AS \"student_id\",\r\nA.ASSESSMENT_NAME AS \"assessment_name\",\r\nA.ASSESSMENT_CODE AS \"assessment_code\",\r\nA.MARK_GRADE AS \"mark_grade\",\r\nA.MARK_NUMERIC AS \"mark_numeric\",\r\nA.MARK_COMMENT AS \"mark_comment\",\r\nA.MARK_CATEGORY AS \"mark_category\",\r\nA.INTENDED_CURRICULUM_GRADE AS \"intended_curriculum_grade\",\r\nA.ASSESSMENT_DATE AS \"assessment_date\",\r\nA.SUBJECT_CODE AS \"subject_code\",\r\nA.SUBJECT_NAME AS \"subject_name\",\r\nY.ACADEMIC_YEAR_CODE AS \"academic_year_code\",\r\nCURRENT_TIMESTAMP() AS \"extraction_date_time\"\r\nFROM ARBOR_BI_CONNECTOR_PRODUCTION.ARBOR_MIS_ENGLAND_MODELLED.STANDARDISED_ASSESSMENT_MARKS A\r\nINNER JOIN ARBOR_BI_CONNECTOR_PRODUCTION.ARBOR_MIS_ENGLAND_MODELLED.ACADEMIC_YEARS Y \r\n          ON Y.APPLICATION_ID = A.APPLICATION_ID AND Y.CALENDAR_YEAR_START_DATE <= A.ASSESSMENT_DATE AND Y.CALENDAR_YEAR_END_DATE >= A.ASSESSMENT_DATE\r\n--WHERE Y.IS_CURRENT = 1\r\nWHERE Y.ACADEMIC_YEAR_CODE IN ('2024-2025', '2025-2026')",
            "exportSettings": {
              "type": "SnowflakeExportCopyCommand"
            },
            "datasetSettings": {
